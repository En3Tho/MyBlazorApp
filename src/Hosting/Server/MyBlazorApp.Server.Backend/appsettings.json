{
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "otlp": {
        "ClusterId": "MyBlazorApp",
        "Match": {
          "Path": "/opentelemetry.proto.collector.trace.v1.TraceService/Export",
          "Methods": [ "POST" ]
        }
      }
    },
    "Clusters": {
      "MyBlazorApp": {
        "Destinations": {
          "Jaeger": {
            "Address": "http://localhost:4317"
          }
        },
        "HttpRequest": {
          "Version": "2",
          "VersionPolicy": "RequestVersionExact"
        }
      }
    }
  }
}
